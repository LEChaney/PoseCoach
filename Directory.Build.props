<?xml version="1.0" encoding="utf-8"?>
<Project>
  <!--
    Pin MSVC toolset for Windows builds generated by CMake/MSBuild.
    This file is auto-imported by MSBuild for all projects under this directory.
    Adjust the version if you install a different compatible 14.44.x toolset.
  -->
  <PropertyGroup Condition="'$(OS)'=='Windows_NT'">
    <PlatformToolset>v143</PlatformToolset>
    <!-- Default unless overridden in environment or higher-level props -->
    <VCToolsVersion Condition="'$(VCToolsVersion)'==''">14.44.35207</VCToolsVersion>
  </PropertyGroup>
</Project>
